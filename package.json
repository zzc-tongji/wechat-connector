  "dependencies": {
    "ajv": "^6.12.2",
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "express": "^4.17.1",
    "hashmap": "^2.4.0",
    "node-fetch": "^2.6.1",
    "wechaty": "^0.40.10"
  },
