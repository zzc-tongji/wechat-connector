{
  "wechaty": {
    "name": "wechat-worker"
  },
  "cache": {
    "expirationSecond": 600,
    "enableLog": false
  },
  "report": {
    "notLoginAfterStart": {
      "timeSecond": 600,
      "maxCount": 3
    },
    "unexpectedLogout": {
      "timeSecond": 120,
      "maxCount": 3
    }
  },
  "mode": "http",
  "http": {
    "receiver": {
      "port": 8002,
      "token": "receiver-token"
    },
    "sender": {
      "id": {
        "server": {
          "url": "http://127.0.0.1:8002/rpc/id",
          "token": "sender-id-token"
        }
      },
      "log": {
        "serverList": [
          {
            "url": "http://127.0.0.1:8002/rpc/log",
            "token": "sender-log-token"
          }
        ]
      }
    }
  }
}
